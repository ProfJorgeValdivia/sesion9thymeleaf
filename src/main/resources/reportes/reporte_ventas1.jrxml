	<queryString language="SQL">
		<![CDATA[SELECT 
    v.id,
    v.fecha,
    v.cliente,
    v.total,
    u.nombre AS usuario_nombre,
    COUNT(d.id) AS cantidad_items
FROM venta v
INNER JOIN usuario u ON u.id = v.usuario_id
LEFT JOIN venta_detalle d ON d.venta_id = v.id
GROUP BY v.id, v.fecha, v.cliente, v.total, u.nombre
ORDER BY v.id DESC;]]>
	</queryString>
